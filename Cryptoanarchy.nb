          "]"}], ",", "block", ",", "block", ",", 
         RowBox[{"{", "1", "}"}], ",", "\"\<X\>\""}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"unique", "=", 
      RowBox[{"Union", "[", "text", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"block", ">", "1"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"code", "=", 
