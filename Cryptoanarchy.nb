   3.605112550557693*^9, 3.6051125824277887`*^9}, {3.605112640936994*^9, 
   3.60511266858784*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"\<\"T\"\>", ",", "\<\"A\"\>", ",", "\<\"N\"\>", ",", "\<\"S\"\>", 
   ",", "\<\"T\"\>", ",", "\<\"A\"\>", ",", "\<\"A\"\>", ",", "\<\"F\"\>", 
